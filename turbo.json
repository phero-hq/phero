{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build:dev": {
      "outputs": ["dist/**"]
    },
    "build:core": {
      "outputs": ["dist/**"]
    },
    "build": {
      "dependsOn": ["^build:dev", "^build:core"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false
    }
  }
}
